.signin_form{class: "hidden"}
  = form_for :user, url: sessions_path, remote: true do |f|
    = f.text_field :user_name, placeholder: 'Enter your username or email'
    = f.password_field :password, placeholder: 'Password'
    = f.submit "Sign In", :class=> "button", id: "login"
    .error_container